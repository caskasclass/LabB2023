<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_LEFT" prefWidth="600.0" styleClass="base" stylesheets="@../css/main.css" type="StackPane" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.homeViewController">
   <children>
      <HBox fx:id="primaryShader" maxHeight="300.0" minHeight="300.0">
         <StackPane.margin>
            <Insets />
         </StackPane.margin></HBox>
      <VBox fx:id="contentContainer" alignment="TOP_CENTER" spacing="10.0">
         <padding>
            <Insets bottom="65.0" left="15.0" right="15.0" top="15.0" />
         </padding>
         <StackPane.margin>
            <Insets top="64.0" />
         </StackPane.margin>
         <children>
            <VBox spacing="5.0">
               <children>
                  <Label text="Le tue Playlist ">
                     <VBox.margin>
                        <Insets bottom="16.0" />
                     </VBox.margin>
                  </Label>
                  <HBox fx:id="playlistBoxContainer" alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" spacing="12.0" styleClass="playlistBox_container" VBox.vgrow="ALWAYS" />
                  <HBox nodeOrientation="RIGHT_TO_LEFT">
                     <children>
                        <Button mnemonicParsing="false" text="see all" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox fx:id="gridContainer">
               <children>
                  <Label text="Album del momento">
                     <VBox.margin>
                        <Insets bottom="16.0" />
                     </VBox.margin></Label>
                  <HBox nodeOrientation="RIGHT_TO_LEFT">
                     <children>
                        <Button mnemonicParsing="false" text="see all" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Titoli del momento ">
                     <VBox.margin>
                        <Insets bottom="16.0" />
                     </VBox.margin>
                  </Label>
                  <HBox fx:id="tableViewContainer" />
                  <HBox nodeOrientation="RIGHT_TO_LEFT">
                     <children>
                        <Button mnemonicParsing="false" text="see all" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox spacing="5.0">
               <children>
                  <Label text="Espolora Playlist ">
                     <VBox.margin>
                        <Insets bottom="16.0" />
                     </VBox.margin>
                  </Label>
                  <HBox fx:id="othersPlaylistBoxContainer" alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" spacing="12.0" styleClass="playlistBox_container" VBox.vgrow="ALWAYS" />
                  <HBox nodeOrientation="RIGHT_TO_LEFT">
                     <children>
                        <Button mnemonicParsing="false" text="see all" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</fx:root>
